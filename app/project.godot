; Engine configuration file.
; It's best edited using the editor UI and not directly,
; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=5

[application]

config/name="exWorlds"
config/features=PackedStringArray("4.3", "GL Compatibility")
config/icon="res://icon.svg"

[autoload]

Globals="*res://scripts/singletons/globals.gd"
ThemeHandler="*res://scripts/singletons/theme_handler.gd"

[internationalization]

locale/translation_remaps={}
locale/translations=PackedStringArray("res://assets/translations/ru_en.English.translation", "res://assets/translations/ru_en.Russian.translation")
locale/fallback="Russian"

[rendering]

renderer/rendering_method="gl_compatibility"
renderer/rendering_method.mobile="gl_compatibility"
